        <ui:Label text="COMPLETE" display-tooltip-when-elided="true" name="complete-label" class="color-complete-bg" style="-unity-text-align: middle-center; height: 20px; -unity-slice-right: 0; width: 100%; white-space: nowrap; visibility: visible; display: flex; -unity-font-style: bold; justify-content: flex-start; align-items: auto;" />
